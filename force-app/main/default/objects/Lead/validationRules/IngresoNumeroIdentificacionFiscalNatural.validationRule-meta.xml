<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IngresoNumeroIdentificacionFiscalNatural</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK( numeroIdentificacionFiscalNatural__c )),NOT(ISNUMBER(numeroIdentificacionFiscalNatural__c )))</errorConditionFormula>
    <errorDisplayField>numeroIdentificacionFiscalNatural__c</errorDisplayField>
    <errorMessage>Colocar sólo valores numéricos</errorMessage>
</ValidationRule>
